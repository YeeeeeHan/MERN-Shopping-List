{
  "name": "mernstack",
  "version": "1.0.0",
  "description": "Shopping list made with MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "hero-postbuild": "NPM_CONFIG_PRODUCT=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Lim Yee Han",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.7",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "prettier": "2.0.2"
  }
}
